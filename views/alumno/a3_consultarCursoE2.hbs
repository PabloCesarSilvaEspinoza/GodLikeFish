                            <div class="col-md-12">
                                <p class="text-center">{{descripcionTarea}}</p>
                                {{#each ../archivosEntregasEstudiante}}
                                    {{#ifEquals idTarea ../this.idTarea}}
                                    <span class="small"><a href="/estudiante/DescargarArchivoEntregado/{{idTarea}}/{{archivoNombre}}" 
                                    class="btn d-inline-block text-truncate btn-rounded btn-sm btn-success small mt-1" style="max-width: 150px;">
                                    <i class="fas fa-download mr-2"></i>{{archivoNombre}}</a></span>
                                    {{/ifEquals}}
                                {{/each}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {{/ifEquals}}
        {{#ifEquals estadoTarea "Pendiente"}}
