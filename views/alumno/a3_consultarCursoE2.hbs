        {{/ifEquals}}
        {{#ifEquals estadoTarea "No entregada"}}
        <div id="tareaNoEntregada{{idTarea}}" class="modal">
            <div class="modal-dialog">
                <div class="modal-content" style="border-radius: 10px">
                    <div class="card-body">
                        <div class="row">
                            <!--titulo-->
                            <div class="col-md-1"></div>
                            <div class="col-md-10 text-center">
                                <h4>{{nombreTarea}}</span></h4>
                                <p class="text-danger">Mensaje de advertencia cruel pero justo</p>
                            </div>
                            <div class="col-md-1 text-right">
                                <button type="button" class="close" data-dismiss="modal"
                                    aria-hidden="true">×</button>
                            </div>
                            <!--contenido-->
                            <div class="col-md-12">
                                <p class="text-center">{{descripcionTarea}}</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        {{/ifEquals}}
    {{/each}}
    <!--Terminan los modales-->
    <div class="row">
        <div class="col-md-9"> <!--Centro-->
            {{#curso}}
