        <div class="modal-dialog modal-xl">
            <div class="modal-content" style="border-radius: 10px">
                <!--Contenido-->
                <div class="modal-body">
                    <div class="row">
                        <!--titulo-->
                        <div class="col-md-3"></div>
                        <div class="col-md-6 text-center">
                            <h4>Perfil de Ponente</h4>
                        </div>
