<div class="container-fluid">
    <!--modales-->
    <div id="editarFoto" class="modal">
        <div class="modal-dialog">
            <div class="modal-content" style="border-radius: 10px">
                <div class="card-body">
                    <div class="row">
                        <!--titulo-->
                        <div class="col-md-3"></div>
                        <div class="col-md-6 text-center">
                            <h4>Editar <span class="small">Foto</span></h4>
                        </div>
                        <div class="col-md-3 text-right">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                        </div>
                        <!--contenido-->
                        <div class="col-md-12">
                            <form action="../editarFoto/{{curso.idCurso}}" method="POST" enctype="multipart/form-data">
                                <div class="row">
                                    <div class="col-md-12">
                                        <input class="mt-2" type="file" name="archivo" multiple />
                                        <input type="hidden" name="idCurso" id="idCurso" value="{{curso.idCurso}}">
                                    </div>
                                    <div class="col-md-12  text-right">
                                        <button type="submit" class="btn btn-info mt-2">
                                            <i class="fas fa-save mr-2"></i>Guardar</button>
                                        <button type="button" class="btn btn-secondary mt-2" data-dismiss="modal">
                                            <i class="fas fa-times mr-2"></i>Cancelar</button>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="editarTemario" class="modal">
        <div class="modal-dialog">
            <div class="modal-content" style="border-radius: 10px">
                <div class="card-body">
                    <div class="row">
                        <!--titulo-->
                        <div class="col-md-3"></div>
                        <div class="col-md-6 text-center">
                            <h4>Editar <span class="small">Temario</span></h4>
                        </div>
                        <div class="col-md-3 text-right">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                        </div>
                        <!--contenido-->
                        <div class="col-md-12">
                            <form action="../editarTemario/{{curso.idCurso}}" method="post" enctype="multipart/form-data">
                                <div class="row">
                                    <div class="col-md-12">
                                        <input class="mt-2" type="file" name="archivo"/>
                                        <input type="hidden" name="idCurso" id="idCurso" value="{{curso.idCurso}}">
                                    </div>
