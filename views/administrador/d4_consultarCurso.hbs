        <div class="modal-dialog">
            <div class="modal-content bg-dark text-white" style="border-radius: 10px">
                <div class="card-body">
                    <div class="row ">
                        <!--titulo-->
                        <div class="col-md-1"></div>
                        <div class="col-md-10  text-center">
                            <h3>Activar <span class="small">Curso</span></h3>
                        </div>
