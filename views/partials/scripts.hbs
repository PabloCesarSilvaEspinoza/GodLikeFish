{{#if graficasAdministrador}}
<script>
        var ctx = document.getElementById('GraficaMuestra');
        {{#valores}}
        var GraficaMuestra = new Chart(ctx, {
            responsive: true,
            type: 'doughnut',
            data: {
                labels: [
                    'Registrados',
                    'Activos',
                    'Inactivos'
                ],
                datasets: [{
                    data: [{{ valor1 }}, {{ valor2 }}, {{ valor3 }}],
        backgroundColor: [
            'rgba(75, 192, 192)',
            'rgb(255, 205, 86)',
            'rgba(255, 99, 132)'
        ],
            hoverOffset: 1
    }]
        },
    options: {
        }
    });
    {{/valores}}
</script>
